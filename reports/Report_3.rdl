  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>Instructor_Courses</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@InstructorID">
            <Value>=Parameters!InstructorID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT 
    C.co_name AS CourseName,
    COUNT(S.st_id) AS StudentCount
FROM 
    Instructor I
    JOIN Instructor_Course IC ON I.ins_id = IC.ins_id
    JOIN Course C ON IC.co_id = C.co_id
    JOIN Track_Course TC ON C.co_id = TC.co_id
    JOIN Track T ON TC.track_id = T.track_id
    JOIN Student S ON T.track_id = S.track_id
WHERE 
    I.ins_id = @InstructorID
GROUP BY 
    C.co_name;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="CourseName">
          <DataField>CourseName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="StudentCount">
          <DataField>StudentCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Instructor_IDS">
      <Query>
        <DataSourceName>Instructor_Courses</DataSourceName>
        <CommandText>SELECT ins_id
FROM Instructor</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="ins_id">
          <DataField>ins_id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
