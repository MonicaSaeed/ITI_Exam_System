  <DataSets>
    <DataSet Name="Students_Information_by_Department">
      <Query>
        <DataSourceName>Students_Information</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@DepartmentNo">
            <Value>=Parameters!DepartmentNo.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT 
    S.st_id AS StudentID,
    S.st_name AS StudentName,
    S.st_email AS StudentEmail,
    S.st_phoneNo AS StudentPhone,
    T.track_name AS TrackName
FROM 
    Student S
    JOIN Track T ON S.track_id = T.track_id
WHERE 
    T.track_id= @DepartmentNo; -- Parameter for Department No</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="StudentID">
          <DataField>StudentID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="StudentName">
          <DataField>StudentName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="StudentEmail">
          <DataField>StudentEmail</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="StudentPhone">
          <DataField>StudentPhone</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="TrackName">
          <DataField>TrackName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Track_Number">
      <Query>
        <DataSourceName>Students_Information</DataSourceName>
        <CommandText>select track_id
from Track</CommandText>
      </Query>
