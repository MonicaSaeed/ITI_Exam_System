  <DataSets>
    <DataSet Name="Topics">
      <Query>
        <DataSourceName>ITI_Exam_System</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@id">
            <Value>=Parameters!id.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT 
	t.topic_name, co.co_name
FROM
	Topic t
INNER JOIN
	Course co ON t.co_id = co.co_id
WHERE
	co.co_id = @id</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="topic_name">
          <DataField>topic_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="co_name">
          <DataField>co_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Courses_ids">
      <Query>
        <DataSourceName>ITI_Exam_System</DataSourceName>
        <CommandText>SELECT 
	co_id
FROM
	Course</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="co_id">
          <DataField>co_id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
