  <DataSets>
    <DataSet Name="Student_Grades_by_Student_ID">
      <Query>
        <DataSourceName>Student_Grades</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StudentID">
            <Value>=Parameters!StudentID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT 
    c.co_name AS Course_Name,
    SUM(CASE WHEN sa.q_id IS NOT NULL THEN q.grade ELSE 0 END) AS Student_Grade,
    SUM(q.grade) AS Total_Grade,
    CAST(
        (SUM(CASE WHEN sa.q_id IS NOT NULL THEN q.grade ELSE 0 END) * 100.0) / SUM(q.grade) 
        AS DECIMAL(5, 2)
    ) AS Grade_Percentage
FROM 
    Student_Exam_Attempt sea
INNER JOIN 
    Exam e ON sea.ex_id = e.ex_id
INNER JOIN 
    Course_Exam ce ON e.ex_id = ce.ex_id
INNER JOIN 
    Course c ON ce.co_id = c.co_id
INNER JOIN 
    Question q ON e.ex_id = q.ex_id
LEFT JOIN 
    Student_Answer sa ON q.q_id = sa.q_id AND sa.st_id = sea.st_id
WHERE 
    sea.st_id = @StudentID 
GROUP BY 
    c.co_name;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Course_Name">
          <DataField>Course_Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Student_Grade">
          <DataField>Student_Grade</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Total_Grade">
          <DataField>Total_Grade</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Grade_Percentage">
          <DataField>Grade_Percentage</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
