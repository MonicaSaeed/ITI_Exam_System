  <DataSources>
    <DataSource Name="Student_Answers">
      <ConnectionProperties>
        <DataProvider>SQL</DataProvider>
        <ConnectString>Data Source=localhost\SQLEXPRESS;Initial Catalog=ExaminationSystem</ConnectString>
        <IntegratedSecurity>true</IntegratedSecurity>
      </ConnectionProperties>
      <rd:SecurityType>Integrated</rd:SecurityType>
      <rd:DataSourceID>1d406014-2f02-48a5-82f8-ea27080e0a25</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="Student_Answers">
      <Query>
        <DataSourceName>Student_Answers</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@s_id">
            <Value>=Parameters!s_id.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@e_id">
            <Value>=Parameters!e_id.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT 
    q.text AS Question_Text,
    ISNULL(STRING_AGG(o.op_text, ', '), 'No Answer') AS Option_Text
FROM 
    Exam e
INNER JOIN 
    Question q ON e.ex_id = q.ex_id
LEFT JOIN
    Student_Answer sa ON sa.q_id = q.q_id AND sa.st_id = @s_id
LEFT JOIN
    [Option] o ON o.op_id = sa.op_id
WHERE
    q.ex_id = @e_id
GROUP BY
    q.text;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Question_Text">
          <DataField>Question_Text</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Option_Text">
          <DataField>Option_Text</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Exams_IDS">
      <Query>
        <DataSourceName>Student_Answers</DataSourceName>
        <CommandText>SELECT DISTINCT ex_id
FROM Student_Exam_Attempt</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="ex_id">
          <DataField>ex_id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Student_IDs">
      <Query>
        <DataSourceName>Student_Answers</DataSourceName>
        <CommandText>SELECT DISTINCT st_id
FROM Student_Exam_Attempt
</CommandText>
      </Query>
      <Fields>
        <Field Name="st_id">
          <DataField>st_id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
